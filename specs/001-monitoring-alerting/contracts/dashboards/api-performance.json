{
    "uid": "api-performance",
    "title": "API Performance",
    "tags": ["api", "performance", "latency"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
        "from": "now-1h",
        "to": "now"
    },
    "templating": {
        "list": [
            {
                "name": "namespace",
                "type": "query",
                "label": "Namespace",
                "query": "label_values(http_request_duration_seconds_count, namespace)",
                "datasource": {
                    "type": "prometheus",
                    "uid": "prometheus"
                },
                "current": {
                    "text": "default",
                    "value": "default"
                },
                "refresh": 1,
                "multi": false,
                "includeAll": false
            }
        ]
    },
    "panels": [
        {
            "id": 1,
            "type": "stat",
            "title": "Request Rate (QPS)",
            "gridPos": {
                "x": 0,
                "y": 0,
                "w": 8,
                "h": 4
            },
            "targets": [
                {
                    "expr": "sum(rate(http_request_duration_seconds_count{namespace=\"$namespace\"}[5m]))",
                    "refId": "A",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                }
            ],
            "options": {
                "graphMode": "area",
                "colorMode": "value",
                "justifyMode": "center",
                "textMode": "auto"
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps",
                    "decimals": 2,
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "value": null,
                                "color": "green"
                            },
                            {
                                "value": 50,
                                "color": "yellow"
                            },
                            {
                                "value": 100,
                                "color": "orange"
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 2,
            "type": "stat",
            "title": "P50 Latency",
            "gridPos": {
                "x": 8,
                "y": 0,
                "w": 8,
                "h": 4
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (le))",
                    "refId": "A",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                }
            ],
            "options": {
                "graphMode": "area",
                "colorMode": "value",
                "justifyMode": "center",
                "textMode": "auto"
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "decimals": 3,
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "value": null,
                                "color": "green"
                            },
                            {
                                "value": 0.2,
                                "color": "yellow"
                            },
                            {
                                "value": 0.5,
                                "color": "red"
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 3,
            "type": "stat",
            "title": "P95 Latency",
            "gridPos": {
                "x": 16,
                "y": 0,
                "w": 8,
                "h": 4
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (le))",
                    "refId": "A",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                }
            ],
            "options": {
                "graphMode": "area",
                "colorMode": "value",
                "justifyMode": "center",
                "textMode": "auto"
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "decimals": 3,
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "value": null,
                                "color": "green"
                            },
                            {
                                "value": 0.5,
                                "color": "yellow"
                            },
                            {
                                "value": 1.0,
                                "color": "red"
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 4,
            "type": "timeseries",
            "title": "Request Rate by Endpoint",
            "gridPos": {
                "x": 0,
                "y": 4,
                "w": 12,
                "h": 8
            },
            "targets": [
                {
                    "expr": "sum(rate(http_request_duration_seconds_count{namespace=\"$namespace\"}[5m])) by (endpoint)",
                    "refId": "A",
                    "legendFormat": "{{endpoint}}",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps",
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "showPoints": "never",
                        "axisPlacement": "auto"
                    }
                }
            }
        },
        {
            "id": 5,
            "type": "timeseries",
            "title": "Response Latency Percentiles",
            "gridPos": {
                "x": 12,
                "y": 4,
                "w": 12,
                "h": 8
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (le))",
                    "refId": "A",
                    "legendFormat": "p50",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                },
                {
                    "expr": "histogram_quantile(0.90, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (le))",
                    "refId": "B",
                    "legendFormat": "p90",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                },
                {
                    "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (le))",
                    "refId": "C",
                    "legendFormat": "p95",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                },
                {
                    "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (le))",
                    "refId": "D",
                    "legendFormat": "p99",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 0,
                        "showPoints": "never",
                        "axisPlacement": "auto"
                    }
                }
            }
        },
        {
            "id": 6,
            "type": "timeseries",
            "title": "Request Duration Heatmap",
            "gridPos": {
                "x": 0,
                "y": 12,
                "w": 24,
                "h": 8
            },
            "targets": [
                {
                    "expr": "sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (le)",
                    "refId": "A",
                    "legendFormat": "{{le}}s",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 30,
                        "showPoints": "never",
                        "axisPlacement": "auto",
                        "stacking": {
                            "mode": "normal"
                        }
                    }
                }
            }
        },
        {
            "id": 7,
            "type": "table",
            "title": "Endpoint Performance Summary",
            "gridPos": {
                "x": 0,
                "y": 20,
                "w": 24,
                "h": 6
            },
            "targets": [
                {
                    "expr": "sum(rate(http_request_duration_seconds_count{namespace=\"$namespace\"}[5m])) by (endpoint)",
                    "refId": "A",
                    "format": "table",
                    "instant": true,
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                },
                {
                    "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (endpoint, le))",
                    "refId": "B",
                    "format": "table",
                    "instant": true,
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                },
                {
                    "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (endpoint, le))",
                    "refId": "C",
                    "format": "table",
                    "instant": true,
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                },
                {
                    "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m])) by (endpoint, le))",
                    "refId": "D",
                    "format": "table",
                    "instant": true,
                    "datasource": {
                        "type": "prometheus",
                        "uid": "prometheus"
                    }
                }
            ],
            "transformations": [
                {
                    "id": "merge"
                },
                {
                    "id": "organize",
                    "options": {
                        "renameByName": {
                            "Value #A": "RPS",
                            "Value #B": "P50 (s)",
                            "Value #C": "P95 (s)",
                            "Value #D": "P99 (s)"
                        }
                    }
                }
            ],
            "options": {
                "showHeader": true,
                "sortBy": [
                    {
                        "displayName": "RPS",
                        "desc": true
                    }
                ]
            },
            "fieldConfig": {
                "overrides": [
                    {
                        "matcher": {
                            "id": "byName",
                            "options": "RPS"
                        },
                        "properties": [
                            {
                                "id": "unit",
                                "value": "reqps"
                            },
                            {
                                "id": "decimals",
                                "value": 2
                            }
                        ]
                    },
                    {
                        "matcher": {
                            "id": "byRegexp",
                            "options": "P[0-9]+.*"
                        },
                        "properties": [
                            {
                                "id": "unit",
                                "value": "s"
                            },
                            {
                                "id": "decimals",
                                "value": 3
                            }
                        ]
                    }
                ]
            }
        }
    ]
}
